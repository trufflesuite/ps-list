{
	"name": "@trufflesuite/ps-list",
	"version": "0.0.1",
	"description": "Get running processes",
	"license": "MIT",
	"repository": "trufflesuite/ps-list",
	"type": "module",
	"contributors": [
		{
			"name": "Sindre Sorhus",
			"email": "sindresorhus@gmail.com",
			"url": "https://sindresorhus.com"
		},
		{
			"name": "Jeff Smale",
			"email": "jeff.smale@consensys.net"
		}
	],
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"xo": {
		"rules": {
			"unicorn/prefer-node-protocol": "off"
		}
	},
	"scripts": {
		"test": "xo && ava && tsd",
		"build:cjs": "npx rollup ./index.js -f cjs -o ./index.cjs"
	},
	"files": [
		"index.cjs",
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"ps",
		"proc",
		"process",
		"processes",
		"list",
		"running",
		"tasklist"
	],
	"dependencies": {
		"tasklist": "^3.1.0"
	},
	"devDependencies": {
		"rollup": "^3.2.3",
		"ava": "^3.15.0",
		"tsd": "^0.18.0",
		"xo": "^0.46.4"
	}
}
